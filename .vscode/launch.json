  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "env": {
        "mongo_conn_str": "mongodb://localhost:27017/CostcoTW_Notify",
        "line_notify_message": "LineNotifyMessage"
      }
    }
  ]
}
